{
    "ci": {
      "collect": {
        "startServerCommand": "npm start",
        "url": ["http://localhost:4000/", "http://localhost:4000/topics/", "http://localhost:4000/resources/", "http://localhost:4000/law-and-regs/", "http://localhost:4000/cases/", "http://localhost:4000/file-a-complaint/"],
        "psiStrategy": "desktop"
      },
      "assert": {
        "assertions": {
          "categories:performance": ["warn", {"minScore": 0.7}],
          "categories:accessibility": ["warn", {"minScore": 1}]
        }
      },
      "upload": {
        "target": "temporary-public-storage"
      }
    }
  }